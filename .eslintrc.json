{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "indent": ["error", 2],
        "lines-between-class-members": ["error", "never"]
        // "skipBlankLines": true
    }
}
